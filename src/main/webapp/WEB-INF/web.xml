<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <listener>
  	<listener-class>listeners.ReservaListener</listener-class>
  </listener>
  <servlet>
  	<servlet-name>ControladorUsuario</servlet-name>
  	<display-name>ControladorUsuario</display-name>
  	<description></description>
  	<servlet-class>controlador.ControladorUsuario</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ControladorEvento</servlet-name>
  	<display-name>ControladorEvento</display-name>
  	<description></description>
  	<servlet-class>controlador.ControladorEvento</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ControladorMensaje</servlet-name>
  	<display-name>ControladorMensaje</display-name>
  	<description></description>
  	<servlet-class>controlador.ControladorMensaje</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ControladorUsuario</servlet-name>
  	<url-pattern>/ControladorUsuario</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ControladorEvento</servlet-name>
  	<url-pattern>/ControladorEvento</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ControladorMensaje</servlet-name>
  	<url-pattern>/ControladorMensaje</url-pattern>
  </servlet-mapping>
</web-app>
